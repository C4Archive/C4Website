CFALog(@"aNumber = %d",1);
CFALog(@"aNumber = %d",2);
CFALog(@"aNumber = %d",3);
CFALog(@"aNumber = %d",4);

CFALog(@"");
CFALog(@"Calls to CFALog() made with FOR loops");
