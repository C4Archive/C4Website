  g<entry>
   <title>Iterative Stretch</title>
   <id>tag:cocoaforartists.org,2011:/examples/Image/ImageStretch/</id>
   <updated>2011-08-19T04:22:58+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Image/ImageStretch/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/993.Image/96.ImageStretch/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

CFAImage *a;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:496];
    a = [CFAImage imageName:@"C4Sky.png"];
}
-(void)draw {
    for(int x = 20, y = 0; x < self.canvasWidth; y += x*0.8, x += x) {
        [a drawAt:NSMakePoint(0, y) withWidth:x andHeight:x*0.8];
        [a drawAt:NSMakePoint(x,y) withWidth:self.canvasWidth-x andHeight:x*0.8];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Dynamic Matrix</title>
   <id>tag:cocoaforartists.org,2011:/examples/Image/DynamicMatrix/</id>
   <updated>2011-08-19T04:23:04+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Image/DynamicMatrix/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/993.Image/95.DynamicMatrix/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

CFAImage *myImage;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:EVENTBASED];
    [self windowWidth:400 andHeight:320];
    myImage = [CFAImage imageName:@"C4Wood.png"];
}
-(void)draw {
    [self background:1.0f];
    int columns = mousePos.x/40;
    if (columns == 0) {
        columns = 1;
    }
    int rows = mousePos.y/32;
    if (rows == 0) {
        rows = 1;
    }
    float colWidth = self.canvasWidth/(float)columns;
    float rowHeight = self.canvasHeight/(float)rows;
    for(int i = 0; i < columns; i++) {
        for(int j = 0; j < rows; j++) {
            [myImage drawAt:NSMakePoint(i*colWidth, j*rowHeight) 
                  withWidth:colWidth andHeight:rowHeight];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
