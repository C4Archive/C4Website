   <title>Random Lines</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/RandomLines/</id>
   <updated>2011-08-19T04:16:54+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/RandomLines/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/94.RandomLines/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [CFAShape strokeWidth:10];
}

-(void)draw {
    for(int i = 0; i < self.canvasWidth; i++){
        float r = FLOAT_FROM_255([CFAMath randomInt:255]);
        float x = [CFAMath randomIntBetweenA:0 andB:self.canvasWidth];
        [CFAShape stroke:r alpha:100];
        [CFAShape lineFromX:i Y:0 toX:x Y:self.canvasHeight];
    }
}
@end]]></script>

          
