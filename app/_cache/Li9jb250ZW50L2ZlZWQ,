  g<entry>
   <title>Simple Curves</title>
   <id>tag:cocoaforartists.org,2011:/examples/Shapes/SimpleCurves/</id>
   <updated>2011-08-19T04:19:02+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Shapes/SimpleCurves/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/984.Shapes/93.SimpleCurves/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas (private)
-(float)sinGraph:(float)value;
-(float)quad:(float)value;
-(float)quadHump:(float)value;
-(float)hump:(float)value;
-(float)squared:(float)value;
@end


@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [CFAShape noFill];
    [CFAShape strokeWidth:1];
}

-(void)draw {
    [CFAShape stroke:0.2f];
    [CFAShape beginShape];
    for(int i = 0; i < self.canvasWidth; i++) {
        float value = [self sinGraph:((float)i/self.canvasWidth)]*self.canvasHeight;
        [CFAShape vertexAtX:i Y:value];
    }
    [CFAShape endShape];
    
    [CFAShape stroke:0.4f];
    [CFAShape beginShape];
    for(int i = 0; i < self.canvasWidth; i++) {
        float value = [self quad:((float)i)/self.canvasWidth]*self.canvasHeight;
        [CFAShape vertexAtX:i Y:value];
    }
    [CFAShape endShape];
    
    [CFAShape stroke:0.6f];
    [CFAShape beginShape];
    for(int i = 0; i < self.canvasWidth; i++) {
        float value = [self quadHump:((float)i)/self.canvasWidth]*self.canvasHeight;
        [CFAShape vertexAtX:i Y:value];
    }
    [CFAShape endShape];
    
    [CFAShape stroke:0.8f];
    [CFAShape beginShape];
    for(int i = 0; i < self.canvasWidth; i++) {
        float value = [self hump:((float)i)/self.canvasWidth]*self.canvasHeight;
        [CFAShape vertexAtX:i Y:value];
    }
    [CFAShape endShape];
    
    [CFAShape stroke:1.0f];
    [CFAShape beginShape];
    for(int i = 0; i < self.canvasWidth; i++) {
        float value = [self squared:((float)i)/self.canvasWidth]*self.canvasHeight;
        [CFAShape vertexAtX:i Y:value];
    }
    [CFAShape endShape];
}

-(float)sinGraph:(float)value {
    value = (value - 0.5) * 1.0; //scale from -1 to 1
    value = [CFAMath sin:(value*PI)]/2 + 0.5;
    return value;
}

-(float)squared:(float)value {
    value = [CFAMath pow:value raiseTo:2];
    // Following line is equal to the above line
    // value = value * value;
    return value;
}

-(float)quad:(float)value {
    value = [self squared:[self squared:value]];
    // Following line is equal to the above line
    // value = value * value * value * value;
    return value;
}

-(float)quadHump:(float)value {
    value = (value - 0.5) * 2; // scale from -2 to 2
    value = [self quad:value] * 16;
    return value;
}

-(float)hump:(float)value {
    value = (value - 0.5) * 2; // scale from -2 to 2
    value = [self squared:value];
    
    if(value > 1) {
        value = 1;
    }
    return 1 - value;
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Sin Cos Curves</title>
   <id>tag:cocoaforartists.org,2011:/examples/Shapes/SinCosCurves/</id>
   <updated>2011-08-19T04:18:41+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Shapes/SinCosCurves/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/984.Shapes/92.SinCosCurves/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
}

-(void)draw {
    [CFAShape noStroke];
    [CFAShape fillRed:1.0f green:0.2f blue:0.2f];
    [CFAShape beginShape];
    for(int x = 0; x <= self.canvasWidth; x+=2) {
        float y = [CFAMath sin:DEGREES_TO_RADIANS(x*360/self.canvasWidth)];
        y *= self.canvasHeight/2.1;
        y += self.canvasHeight/2;
        [CFAShape vertexAt:NSMakePoint(x, y)];
    }
    [CFAShape endShape];
    
    [CFAShape fillRed:0.2f green:0.4f blue:1.0f];
    [CFAShape beginShape];
    for(int x = 0; x <=self.canvasWidth; x+=2) {
        float y = [CFAMath sin:DEGREES_TO_RADIANS(x*360/self.canvasWidth)];
        y *= self.canvasHeight/6;
        y += self.canvasHeight/2;
        [CFAShape vertexAt:NSMakePoint(x/2, y)];
    }
    [CFAShape endShape];
    
    [CFAShape fillRed:0.3f green:0.3f blue:0.3f];
    [CFAShape beginShape];
    for(int x = 0; x <=self.canvasWidth/2; x+=2) {
        if(x >= self.canvasWidth/2) [CFAShape stroke:0];
        float y = [CFAMath cos:DEGREES_TO_RADIANS(x*2*360/self.canvasWidth)];
        y *= -self.canvasHeight/6;
        y += self.canvasHeight*2/3;
        [CFAShape vertexAt:NSMakePoint(x+self.canvasWidth/2, y)];
    }
    [CFAShape endShape];
}
@end
]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Pie Chart</title>
   <id>tag:cocoaforartists.org,2011:/examples/Shapes/PieChart/</id>
   <updated>2011-08-19T04:18:45+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Shapes/PieChart/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/984.Shapes/91.PieChart/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
NSObject *o = [[[NSObject alloc] init] retain];
#import "CFACanvas.h"

int diameter = 150;
int angs[] = {30,10,45,35,60,38,75,67};
float lastAng = 0;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [CFAShape noStroke];
}

-(void)draw {
    
    for(int i = 0; i < 8; i++) {
        [CFAShape fill:FLOAT_FROM_255(angs[i]*3)];
        [CFAShape arcWithCenterAt:self.centrePos 
                           radius:diameter/2 
                       startAngle:lastAng 
                         endAngle:lastAng+angs[i]+5];
        lastAng += angs[i];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

