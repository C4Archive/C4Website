#import "CFACanvas.h"

@interface CFACanvas () {}
-(void)setupC4Points;
@end

@implementation CFACanvas

NSPoint points[371];
NSInteger currentPointNumber, cCount, fourCount, squareCount;

-(void)setup {
    
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [self setupC4Points];
    [CFAShape noStroke];
    
    currentPointNumber = 0;
    cCount = 0;
    fourCount = 152;
    squareCount = 335;
}

-(void)draw {
    [self background:0 alpha:0.066];
    
    [CFAShape fillRed:1.0f green:0.2f blue:0.2f];
    [CFAShape circleAt:points[cCount] radius:2];
    [CFAShape circleAt:points[151-cCount] radius:2];
    //    [CFAShape pointAt:points[cCount]];
    //    [CFAShape pointAt:points[151-cCount]];
    
    [CFAShape fillRed:0.2f green:0.4f blue:1.0f];
    [CFAShape circleAt:points[fourCount] radius:2];
    [CFAShape circleAt:points[325-fourCount+152] radius:2];
    //    [CFAShape pointAt:points[fourCount]];
    //    [CFAShape pointAt:points[325-fourCount+152]];
    
    [CFAShape fillRed:0.3f green:0.3f blue:0.3f];
    [CFAShape circleAt:points[squareCount] radius:2];
    [CFAShape circleAt:points[370-squareCount+335] radius:2];
    //    [CFAShape pointAt:points[squareCount]];
    //    [CFAShape pointAt:points[371-squareCount+335]];
    
    currentPointNumber++;
    cCount++;
    fourCount++;
    squareCount++;
    if(cCount == 152) cCount = 0;
    if(fourCount == 326) fourCount = 152;
    if(squareCount == 371) squareCount = 335;
    if(currentPointNumber == 371) currentPointNumber = 0;
}

//Gives values to each point in the array
