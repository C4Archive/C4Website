#import "CFACanvas.h"

@interface CFACanvas () {}
-(void)setupC4Points;
@end

@implementation CFACanvas

NSPoint points[371];
NSInteger currentPointNumber;

-(void)setup {
    
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [self setupC4Points];
    [CFAShape noStroke];
    
    currentPointNumber = 0;
}

-(void)draw {
    [self background:0 alpha:0.05];
    
    if (currentPointNumber == 0) {
        [CFAShape fillRed:1.0f green:0.2f blue:0.2f];
    }
    else if (currentPointNumber == 152) {
        [CFAShape fillRed:0.2f green:0.4f blue:1.0f];
    }
    else if (currentPointNumber == 335) {
        [CFAShape fillRed:0.3f green:0.3f blue:0.3f];
    }
    
    [CFAShape circleAt:points[currentPointNumber] radius:2];
    //    [CFAShape pointAt:points[currentPointNumber]];
    
    currentPointNumber++;
    if(currentPointNumber == 372) 
        currentPointNumber = 0;
}

//Gives values to each point in the array
