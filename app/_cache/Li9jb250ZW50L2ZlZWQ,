  g<entry>
   <title>Background Image</title>
   <id>tag:cocoaforartists.org,2011:/examples/Image/BackgroundImage/</id>
   <updated>2011-08-19T04:21:50+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Image/BackgroundImage/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/993.Image/98.BackgroundImage/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

CFAImage *bg;
int y;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self drawStyle:DISPLAYRATE];
    
    y = 0;
    bg = [CFAImage imageName:@"C4Sky" andType:@"png"];
    [CFAShape stroke:1.0f];
}
-(void)draw {
    [self backgroundImage:bg];
    y ++;
    if(y > self.canvasHeight) y = 0;
    
    [CFAShape lineFromX:0 Y:y toX:self.canvasWidth Y:y];
}
@end]]></script>

          
