  g<entry>
   <title>C4 Random Lines</title>
   <id>tag:cocoaforartists.org,2011:/examples/C4/C4RandomLines/</id>
   <updated>2011-08-19T04:14:40+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/C4/C4RandomLines/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/999.C4/93.C4RandomLines/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

NSPoint points[371];

@interface CFACanvas () {}
-(void)setupC4Points;
-(void)drawRandomLines;
@end

@implementation CFACanvas
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [CFAShape noFill];
    [CFAShape stroke];
    [self setupC4Points];
}

-(void)draw {
    [self background:0 alpha:0.066];
    [self drawRandomLines];
}

-(void)drawRandomLines {
    [CFAShape strokeRed:1.0f green:0.2f blue:0.2f];
    for(int i = 0; i < 10; i++) {
        NSInteger random = [CFAMath randomInt:152];
        NSPoint p = points[random];
        [CFAShape lineFromX:mousePos.x Y:mousePos.y toX:p.x Y:p.y];
    }
    
    [CFAShape strokeRed:0.2f green:0.4f blue:1.0f];
    for(int i = 0; i < 10; i++) {
        NSInteger random = [CFAMath randomInt:(334-152)]+152;
        NSPoint p = points[random];
        [CFAShape lineFromX:mousePos.x Y:mousePos.y toX:p.x Y:p.y];
    }
    
    [CFAShape strokeRed:0.3f green:0.3f blue:0.3f];
    for(int i = 0; i < 5; i++) {
        NSInteger random = [CFAMath randomInt:(371-335)]+335;
        NSPoint p = points[random];
        [CFAShape lineFromX:mousePos.x Y:mousePos.y toX:p.x Y:p.y];
    }
}

-(void)setupC4Points {
    //C
    points[0].x = 80.00; points[0].y = 80.00;
