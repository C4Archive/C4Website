  g<entry>
   <title>Random Font</title>
   <id>tag:cocoaforartists.org,2011:/examples/Font/RandomFont/</id>
   <updated>2011-08-19T04:15:16+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Font/RandomFont/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/994.Font/98.RandomFont/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import <Cocoa/Cocoa.h>

@implementation CFACanvas

NSArray *allFontNames;

-(void)setup {
    [self windowWidth:400 andHeight:320];
    
    allFontNames = [[CFAFont availableFonts] retain];
}

-(void)draw {
    [self background:0];
    
    NSInteger rand = [CFAMath randomInt:[allFontNames count]];
    
    CFAString *fontName = [allFontNames objectAtIndex:rand];
    CFAFont *currentFont = [CFAFont fontWithName:fontName size:16];
    
    [CFAString font:currentFont];
    [CFAString fill:1.0f];
    
    NSPoint drawPoint = NSMakePoint(20, self.centerPos.y - 2* [currentFont ascender]);
    
    [[CFAString stringWithFormat:
      @"Current Font: %@ \nFamily Name: %@ \nDisplay Name: %@ \n#%d of %d", 
      fontName, [currentFont familyName], 
      [currentFont displayName], 
      rand, [allFontNames count]] drawAtPoint:drawPoint];
}

-(void)mousePressed {
    [self redraw];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Your Fonts</title>
   <id>tag:cocoaforartists.org,2011:/examples/Font/YourFonts/</id>
   <updated>2011-08-19T04:15:13+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Font/YourFonts/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/994.Font/97.YourFonts/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import <Cocoa/Cocoa.h>

@implementation CFACanvas

CFAFont *yourUserFont, *yourBoldFont, *yourMessageFont, *yourSystemFont;

-(void)setup {
    [self windowWidth:400 andHeight:320];
    
    yourUserFont = [[CFAFont userFontOfSize:16] retain];
    yourBoldFont = [[CFAFont boldSystemFontOfSize:16] retain];
    yourMessageFont = [[CFAFont messageFontOfSize:16] retain];
    yourSystemFont = [[CFAFont systemFontOfSize:16] retain];
}

-(void)draw {
    [CFAString fill:1.0f];
    
    [CFAString font:yourUserFont];    
    CFAString *currentString = [CFAString stringWithFormat:
                                @"Your User Font is: %@", [yourUserFont fontName]];
    [currentString drawAtPoint:NSMakePoint(20, self.canvasHeight/5*4)];
    
    [CFAString font:yourBoldFont];
    currentString = [CFAString stringWithFormat:
                     @"Your Bold Font is: %@", [yourBoldFont fontName]];
    [currentString drawAtPoint:NSMakePoint(20, self.canvasHeight/5*3)];
    
    [CFAString font:yourMessageFont];
    currentString = [CFAString stringWithFormat:
                     @"Your Message Font is: %@", [yourMessageFont fontName]];
    [currentString drawAtPoint:NSMakePoint(20, self.canvasHeight/5*2)];
    
    [CFAString font:yourSystemFont];
    currentString = [CFAString stringWithFormat:
                     @"Your System Font is: %@", [yourSystemFont fontName]];
    [currentString drawAtPoint:NSMakePoint(20, self.canvasHeight/5*1)];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

