            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

@implementation CFACanvas

-(void)setup {
    [self windowWidth:400 andHeight:320];
}

-(void)draw {
    //draw a C
    NSPoint startingPoint, currentPoint;
    [CFAShape fillRed:1.0f green:0.2f blue:0.2f];
    startingPoint = NSMakePoint(80, 80);
    
    for(int i = 0; i <= 32; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y + i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 24; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x - i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 24; i++) {
        currentPoint.x = startingPoint.x - i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    //Draw a 4
    [CFAShape fillRed:0.2f green:0.4f blue:1.0f];
    
    startingPoint = NSMakePoint(240,80);
    currentPoint = startingPoint;
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y + i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x - i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 24; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y + i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y + i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 16; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }    
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }    
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x - i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }    
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }    
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x - i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }    
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y + i*5;
        [CFAShape pointAt:currentPoint];
    }    
    
    
    //Draw A Square
    [CFAShape fillRed:0.3f green:0.3f blue:0.3f];
    startingPoint = NSMakePoint(160, 240);
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x + i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y - i*5;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x - i*5;
        currentPoint.y = startingPoint.y;
        [CFAShape pointAt:currentPoint];
    }
    
    startingPoint.x = currentPoint.x;
    startingPoint.y = currentPoint.y;
    
    for(int i = 0; i <= 8; i++) {
        currentPoint.x = startingPoint.x;
        currentPoint.y = startingPoint.y + i*5;
        [CFAShape pointAt:currentPoint];
    }    
}
@end]]></script>

          
