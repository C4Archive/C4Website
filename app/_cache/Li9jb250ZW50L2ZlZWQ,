  g<entry>
   <title>Noise 2D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Noise/Noise2D/</id>
   <updated>2011-08-19T04:21:19+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Noise/Noise2D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/989.Noise/98.Noise2D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float increment;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    increment = [CFAMath randomInt:30]/1000.0f;
}

-(void)draw {
    float xOff = 0.0f;
    for(int x = 0; x < self.canvasWidth; x++) {
        xOff += increment;
        float yOff = 0.0f;
        for(int y = 0; y < self.canvasHeight; y++) {
            yOff += increment;
            float bright = [CFANoise noiseX:xOff Y:yOff];
            [CFAShape fill:[CFAMath absf:bright]];
            [CFAShape pointAtX:x Y:y];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Noise 3D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Noise/Noise3D/</id>
   <updated>2011-08-19T04:21:27+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Noise/Noise3D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/989.Noise/97.Noise3D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float increment, zincrement, zOff;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    zOff = 0.0f;
    zincrement = 0.02f;
    increment = 0.02f;
}

-(void)draw {
    [self background:0];
    float xOff = 0.0f;
    for(int x = 0; x < self.canvasWidth; x+=2) {
        xOff += increment;
        float yOff = 0.0f;
        for(int y = 0; y < self.canvasHeight; y+=2) {
            yOff += increment;
            float bright = [CFANoise noiseX:xOff Y:yOff Z:zOff]+0.2;
            [CFAShape fill:bright];
            [CFAShape pointAtX:x Y:y];
        }
    }
    zOff += zincrement; // Increment zoff
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Noise Wave</title>
   <id>tag:cocoaforartists.org,2011:/examples/Noise/NoiseWave/</id>
   <updated>2011-08-19T04:21:22+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Noise/NoiseWave/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/989.Noise/96.NoiseWave/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float width, xSpacing, yOff;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    width = self.canvasWidth+16;
    xSpacing = 8;
    yOff = 0.0f;
    [CFAShape fill:1 alpha:0.2];
    [CFAShape noStroke];
}

-(void)draw {
    [self background:0];
    float dx = 0.05f;
    float dy = 0.01f;
    yOff += dy;
    float xOff = yOff;
    NSPoint location;
    for(int i = 0; i < width/xSpacing; i++) {
        location.x = i*xSpacing;
        location.y = (2*[CFANoise noiseX:xOff])*self.canvasHeight/2+self.canvasHeight/2;
        xOff+=dx;
        [CFAShape circleAt:location radius:16];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

  g<entry>
   <title>Numbers</title>
   <id>tag:cocoaforartists.org,2011:/examples/Number/</id>
