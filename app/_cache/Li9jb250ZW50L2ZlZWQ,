  g<entry>
   <title>Double Random</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/DoubleRandom/</id>
   <updated>2011-08-19T04:17:17+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/DoubleRandom/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/93.DoubleRandom/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int totalPoints;
float steps;
int randomValue;

@implementation CFACanvas 
-(void)setup {
    [self drawStyle:EVENTBASED];
    [self windowWidth:400 andHeight:320];
    [CFAShape fill:1.0f];
    
    totalPoints = self.canvasWidth;
    steps = totalPoints + 1;
    randomValue = 0;
    [self background:0];
}

-(void)draw {
    randomValue = 0;
    [self background:0];
    for(int i = 1; i < steps; i++){
        int mod = 0;
        while (mod == 0) {
            mod = [CFAMath randomIntBetweenA:-1 andB:1];
        }
        [CFAShape pointAtX:self.canvasWidth/steps*i 
                         Y:self.canvasHeight/2 + mod*randomValue];
        randomValue += [CFAMath randomIntBetweenA:-10 andB:10];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>SinCosTan</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/SinCosTan/</id>
   <updated>2011-08-19T04:17:01+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/SinCosTan/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/92.SinCosTan/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

@interface CFACanvas () {}
-(CGFloat)sinY;
-(CGFloat)cosY;
-(CGFloat)tanY;
@end

NSInteger x;
NSInteger radius;

@implementation CFACanvas 
-(void)setup {
[self drawStyle:DISPLAYRATE];
[self windowWidth:400 andHeight:320];
[CFAShape noStroke];
radius = 5;
x = -10;
}

-(void)draw {
[self background:0 alpha:0.025];
x++;
if(x > self.canvasWidth+radius) x = -1*radius;

[CFAShape fillRed:1.0f green:0.2f blue:0.2f];
[CFAShape circleAt:NSMakePoint(x, [self sinY]) radius:radius];

[CFAShape fillRed:0.2f green:0.4f blue:1.0f];
[CFAShape circleAt:NSMakePoint(x, [self cosY]) radius:radius];

[CFAShape fill:0.3f];
[CFAShape circleAt:NSMakePoint(x, [self tanY]) radius:radius];
}

-(CGFloat)sinY {
CGFloat theta = [CFAMath map:x 
fromMin:0 
max:self.canvasWidth 
toMin:0 
max:2*PI];
CGFloat radianY = [CFAMath sin:theta];
return [CFAMath map:radianY fromMin:-1 max:1 toMin:0 max:self.canvasHeight];
}

-(CGFloat)cosY {
CGFloat theta = [CFAMath map:x 
fromMin:0 
max:self.canvasWidth 
toMin:0 
max:2*PI];
CGFloat radianY = [CFAMath cos:theta];
return [CFAMath map:radianY fromMin:-1 max:1 toMin:0 max:self.canvasHeight];
}

-(CGFloat)tanY {
CGFloat theta = [CFAMath map:x 
fromMin:0 
max:self.canvasWidth 
toMin:0 
max:2*PI];
CGFloat radianY = [CFAMath tan:theta];
return [CFAMath map:radianY fromMin:-1 max:1 toMin:0 max:self.canvasHeight];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Mixed Sine Wave</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/MixedSineWave/</id>
   <updated>2011-08-19T04:17:08+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/MixedSineWave/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/91.MixedSineWave/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas (private)
-(void)calcWave;
-(void)renderWave;
@end

int xspacing = 8;
int w = 416;
int maxwaves = 6;
float theta = 0.0f;
float amplitude[6];
float dx[6];
float yValueCount = 51;
float yValues[51];

@implementation CFACanvas
-(void)setup {
    [self drawStyle:ANIMATED];
    [self windowWidth:400 andHeight:320];
    for(int i = 0; i < maxwaves; i++) {
        amplitude[i] = [CFAMath randomIntBetweenA:10 andB:30];
        float period = [CFAMath randomIntBetweenA:100 andB:300];
        dx[i] = (TWO_PI / (float)period) * xspacing;
    }
}
-(void)draw {
    [self background:0];
    [self calcWave];
    [self renderWave];
}
-(void)calcWave {
    theta += 0.02;
    for(int i = 0; i < yValueCount; i++) {
        yValues[i] = 0;
    }
    for(int j = 0; j < maxwaves; j++) {
        float x = theta;
        for(int i = 0; i < yValueCount; i++) {
            if(j%2 == 0){
                yValues[i] += [CFAMath sin:x]*amplitude[j];
            }
            else {
                yValues[i] += [CFAMath cos:x]*amplitude[j];
            }
            x += dx[j];
        }
    }
}
-(void)renderWave {
    [CFAShape noStroke];
    [CFAShape fill:1.0f alpha:0.6f];
    for(int x = 0; x < yValueCount; x++) {
        [CFAShape ellipseWithXPos:x*xspacing yPos:self.canvasWidth/2+yValues[x] width:16 andHeight:16];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Polar Coordinates</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/PolarCoords/</id>
   <updated>2011-08-19T04:16:57+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/PolarCoords/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/90.PolarCoords/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float radius;
float theta;
float theta_vel;
float theta_acc;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    radius = 50.0f;
    theta = 0.0f;
    theta_vel = 0.0f;
    theta_acc = 0.0001f;
}

-(void)draw {
    [self background:0 alpha:0.1f];
    [CFAShape fill:0.8f];
    
    NSPoint p1 = NSMakePoint(radius*[CFAMath cos:theta] + self.canvasWidth/2,
                             radius*[CFAMath sin:theta] + self.canvasHeight/2);
    
    NSPoint p2 = NSMakePoint(radius*2*[CFAMath cos:-theta] + self.canvasWidth/2,
                             radius*2*[CFAMath sin:-theta] + self.canvasHeight/2);
    
    [CFAShape circleAt:p1 radius:8];
    [CFAShape circleAt:p2 radius:16];
    theta_vel += theta_acc;
    theta += theta_vel;
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

