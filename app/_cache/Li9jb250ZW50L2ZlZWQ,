    g<entry>
   <title>Array 1D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/Array1D/</id>
   <updated>2011-08-19T04:15:38+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/Array1D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/99.Array1D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float cosWave[400];

@implementation CFACanvas
-(void)setup {
    [CFAShape stroke];
    [self windowWidth:400 andHeight:320];
    for(int i = 0; i < self.canvasWidth; i++) {
        float amount = [CFAMath map:i fromMin:0 max:400 toMin:0 max:PI];
        cosWave[i] = [CFAMath absf:[CFAMath cos:amount]];
    }
}
-(void)draw {
    [self background:0];
    for(int i = 0; i < self.canvasWidth; i++) {
        [CFAShape stroke:cosWave[i]];
        [CFAShape lineFromX:i Y:0 toX:i Y:self.canvasHeight/3];
    }
    for(int i = 0; i < self.canvasWidth; i++) {
        [CFAShape stroke:cosWave[i]/4];
        [CFAShape lineFromX:i Y:self.canvasHeight/3 toX:i Y:self.canvasHeight/3*2];
    }
    for(int i = 0; i < self.canvasWidth; i++) {
        [CFAShape stroke:1-cosWave[i]];
        [CFAShape lineFromX:i Y:self.canvasHeight/3*2 toX:i Y:self.canvasHeight];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Array 2D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/Array2D/</id>
   <updated>2011-08-19T04:15:35+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/Array2D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/98.Array2D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int width = 400;
int height = 320;
float distances[400][320];
NSPoint center;
float maxDistance;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:width andHeight:height];
    [self background:0];
    center = NSMakePoint(width/2, height/2);
    maxDistance = [CFAVector distanceBetweenA:NSZeroPoint andB:center];
    for(int j = 0; j < width; j++){
        for(int i = 0; i < height; i++){
            float dist = [CFAVector distanceBetweenA:NSMakePoint(j,i) andB:center];
            distances[j][i] = dist/maxDistance;
        }
    }
}
-(void)draw {
    [self background:0];
    for(int j = 0; j < width; j+=2) {
        for(int i = 0; i < height; i+=2) {
            [CFAShape fill:distances[j][i]];
            [CFAShape pointAtX:j Y:i];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>C Arrays</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/CArray/</id>
   <updated>2011-08-19T04:15:31+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/CArray/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/97.CArray/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
}

-(void)draw {
    //Create two arrays
    CFAString *stringArray[5];
    NSArray *stringObjectArray;
    
    //Create some objects
    CFAString *s0, *s1, *s2, *s3, *s4;
    s0 = [CFAString stringWithFormat:@"string 0"];
    s1 = [CFAString stringWithFormat:@"string 1"];
    s2 = [CFAString stringWithFormat:@"string 2"];
    s3 = [CFAString stringWithFormat:@"string 3"];
    s4 = [CFAString stringWithFormat:@"string 4"];
    
    CFAString *cArray = [CFAString stringWithString:@"THE C ARRAY"];
    CFAString *nsArray = [CFAString stringWithString:@"THE NSARRAY"];
    
    [CFAString fill:1.0f];
    
    //Load the arrays
    stringArray[0] = s0;
    stringArray[1] = s1;
    stringArray[2] = s2;
    stringArray[3] = s3;
    stringArray[4] = s4;
    
    //nil indicates that the list is finished
    stringObjectArray = [NSArray arrayWithObjects:s0,s1,s2,s3,s4,nil]; 
    
    CFALog(@"The C Array");
    // we need to remember how many objects are in the C Array ourselves
    for(int i = 0; i < 5; i++) { 
        CFALog(@"%@",stringArray[i]);
    }
    
    NSPoint p = NSMakePoint(20, self.canvasHeight-32);
    
    [cArray drawAtPoint:p];
    p.y -= 20;
    [s0 drawAtPoint:p];
    p.y -= 16;
    [s1 drawAtPoint:p];
    p.y -= 16;
    [s2 drawAtPoint:p];
    p.y -= 16;
    [s3 drawAtPoint:p];
    p.y -= 16;
    [s4 drawAtPoint:p];
    p.y -= 32;
    
    CFALog(@"The NSArray");
    //notice the COUNT method? we cannot call this on the C Array
    for(int i = 0; i < [stringObjectArray count]; i++) {  
        CFALog(@"%@",[stringObjectArray objectAtIndex:i]);
    }
    
    [nsArray drawAtPoint:p];
    p.y -= 20;
    [(CFAString *)[stringObjectArray objectAtIndex:0] drawAtPoint:p];
    p.y -= 16;
    [(CFAString *)[stringObjectArray objectAtIndex:1] drawAtPoint:p];
    p.y -= 16;
    [(CFAString *)[stringObjectArray objectAtIndex:2] drawAtPoint:p];
    p.y -= 16;
    [(CFAString *)[stringObjectArray objectAtIndex:3] drawAtPoint:p];
    p.y -= 16;
    [(CFAString *)[stringObjectArray objectAtIndex:4] drawAtPoint:p];
    p.y -= 16;
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Object Arrays</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/ObjectArray/</id>
   <updated>2011-08-19T04:15:24+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/ObjectArray/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/96.ObjectArray/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
}

-(void)draw {
    //Let's create a bunch of objects
    CFAColor *c = [CFAColor colorWithGrey:.66];
    CFAFont *f = [CFAFont fontWithName:@"Arial" size:20];
    CFAImage *i = [CFAImage imageName:@"C4Red.png"];
    CFAString *s = [CFAString stringWithString:@"a string"];
    CFAVoice *v = [CFAVoice initWithVoice:ALEX];
    CFAVector *vec = [CFAVector vectorWithX:0 Y:1 Z:0];
    CFAObject *o = [[[CFAObject alloc] init] retain];
    
    NSArray *array0, *array1, *array2;
    
    CFALog(@"Create an array with one object");
    array0 = [NSArray arrayWithObject:c];
    CFALog(@"%@",array0);
    
    CFALog(@"Create an array with multiple objects");
    array1 = [NSArray arrayWithObjects:c,f,i,s,v,vec,o,nil];
    CFALog(@"%@",array1);
    
    CFALog(@"Create an array with object from another array");
    array2 = [NSArray arrayWithArray:array1];
    CFALog(@"%@",array2);
    
    NSPoint p = NSMakePoint(20, self.canvasHeight-32);
    [CFAString fill:1];
    [[CFAString stringWithString:@"The Array"] drawAtPoint:p];
    p.y -= 172;
    [[CFAString stringWithFormat:@"%@",array2] drawAtPoint:p];
    
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Object Array 1D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/ObjectArray1D/</id>
   <updated>2011-08-19T04:15:20+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/ObjectArray1D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/95.ObjectArray1D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

NSMutableArray *cosWave;

@implementation CFACanvas
-(void)setup {
    [CFAShape stroke];
    [self flipCoordinates];
    [self windowWidth:400 andHeight:320];
    
    cosWave = [[NSMutableArray arrayWithCapacity:0] retain];
    
    for(int i = 0; i < self.canvasWidth; i++) {
        float amount = [CFAMath map:i fromMin:0 max:400 toMin:0 max:PI];
        
        // because we're working with an object array, we need to first create a number object
        // see the NumberObject example, in Code Samples, for more about these
        NSNumber *n = [NSNumber numberWithFloat:[CFAMath absf:[CFAMath cos:amount]]];
        [cosWave addObject:n];
    }
}

-(void)draw {
    [self background:0];
    for(int i = 0; i < self.canvasWidth; i++) {
        [CFAShape stroke:[[cosWave objectAtIndex:i] floatValue]];
        [CFAShape lineFromX:i Y:0 toX:i Y:self.canvasHeight/3];
    }
    for(int i = 0; i < self.canvasWidth; i++) {
        [CFAShape stroke:[[cosWave objectAtIndex:i] floatValue]/4];
        [CFAShape lineFromX:i Y:self.canvasHeight/3 toX:i Y:self.canvasHeight/3*2];
    }
    for(int i = 0; i < self.canvasWidth; i++) {
        [CFAShape stroke:1-[[cosWave objectAtIndex:i] floatValue]];
        [CFAShape lineFromX:i Y:self.canvasHeight/3*2 toX:i Y:self.canvasHeight];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Object Array 2D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/ObjectArray2D/</id>
   <updated>2011-08-19T04:15:41+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/ObjectArray2D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/94.ObjectArray2D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

NSMutableArray *distances;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0];
    NSPoint center = NSMakePoint(self.canvasWidth/2, self.canvasHeight/2);
    float maxDistance = [CFAVector distanceBetweenA:NSZeroPoint andB:center];
    distances = [[NSMutableArray arrayWithCapacity:0] retain];
    for(int i = 0; i < self.canvasHeight; i++){
        NSMutableArray *rowOfDistances = [NSMutableArray arrayWithCapacity:0];
        for(int j = 0; j < self.canvasWidth; j++){
            float dist = [CFAVector distanceBetweenA:NSMakePoint(j, i) andB:center];
            [rowOfDistances addObject:[NSNumber numberWithFloat:dist/maxDistance]];
        }
        [distances addObject:rowOfDistances];
    }
}

-(void)draw {
    [self background:0];
    for(int i = 0; i < [distances count]; i+=2) {
        for(int j = 0; j < [[distances objectAtIndex:i] count]; j+=2) {
            float color = [[[distances objectAtIndex:i] objectAtIndex:j] floatValue];
            [CFAShape fill:color];
            [CFAShape pointAtX:j Y:i];
            //A Different Drawing Scheme
            //[CFAShape pointAtX:j/2+self.canvasWidth/4 Y:i/2+self.canvasHeight/4];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Mutable Arrays</title>
   <id>tag:cocoaforartists.org,2011:/examples/Arrays/MutableArray/</id>
   <updated>2011-08-19T04:15:27+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Arrays/MutableArray/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1000.Arrays/93.MutableArray/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
}

-(void)draw {
    [self background:0];
    [CFAString fill:1.0f];
    /*
     The NSMutableArray stores lists of objects which can be changed...
     Objects can be of any kind (class)...
     */
    
    //Let's create a bunch of objects
    CFAColor *c = [CFAColor colorWithGrey:.66];
    CFAFont *f = [CFAFont fontWithName:@"Arial" size:20];
    CFAImage *i = [CFAImage imageName:@"C4Red.png"];
    CFAString *s = [CFAString stringWithString:@"a string"];
    CFAVoice *v = [CFAVoice initWithVoice:ALEX];
    CFAVector *vec = [CFAVector vectorWithX:0 Y:1 Z:0];
    CFAObject *o = [[[CFAObject alloc] init] retain];
    
    NSMutableArray *array0, *array1, *array2;
    CFALog(@"------------------------------------------------");
    CFALog(@"Create an array, add one object");
    array0 = [NSMutableArray arrayWithCapacity:0];
    [array0 addObject:c];
    CFALog(@"%@ \n",array0);
    
    CFAString *currentString = [CFAString stringWithString:@"Initial Array"];
    
    NSPoint p = NSMakePoint(20, self.canvasHeight-32);
    [currentString drawAtPoint:p];
    
    currentString = [CFAString stringWithFormat:@"%@",array0];
    p.y -= 60;
    [currentString drawAtPoint:p];
    
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Add another object");
    [array0 addObject:f];
    CFALog(@"%@ \n",array0);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Create an array, add multiple objects");
    array1 = [NSMutableArray arrayWithCapacity:0];
    [array1 addObject:c];
    [array1 addObject:f];
    [array1 addObject:i];
    [array1 addObject:s];
    [array1 addObject:v];
    [array1 addObject:vec];
    [array1 addObject:o];
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Access various objects");
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"object 1: %@",[array1 objectAtIndex:0]);
    CFALog(@"object 3: %@",[array1 objectAtIndex:3]);
    CFALog(@"last obj: %@ \n",[array1 lastObject]);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Remove a specific object");
    [array1 removeObjectAtIndex:0];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"object 1: %@",[array1 objectAtIndex:0]);
    CFALog(@"object 3: %@",[array1 objectAtIndex:3]);
    CFALog(@"last obj: %@ \n",[array1 lastObject]);
    
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Remove last object");
    [array1 removeLastObject];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"object 1: %@",[array1 objectAtIndex:0]);
    CFALog(@"object 3: %@",[array1 objectAtIndex:3]);
    CFALog(@"last obj: %@ \n",[array1 lastObject]);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Add object");
    [array1 addObject:c];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"object 1: %@",[array1 objectAtIndex:0]);
    CFALog(@"object 3: %@",[array1 objectAtIndex:3]);
    CFALog(@"last obj: %@ \n",[array1 lastObject]);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Add objects");
    [array1 addObject:c];
    [array1 addObject:c];
    [array1 addObject:c];
    [array1 addObject:c];
    [array1 addObject:c];
    [array1 addObject:c];
    [array1 addObject:c];
    [array1 addObject:c];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Add object at index");
    [array1 insertObject:vec atIndex:10];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Replacing an object");
    [array1 replaceObjectAtIndex:10 withObject:o];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Exchanging an object");
    [array1 exchangeObjectAtIndex:10 withObjectAtIndex:0];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Remove Identical Objects");
    [array1 removeObjectIdenticalTo:c];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Adding Objects From Another Array");
    [array1 addObjectsFromArray:array0];
    CFALog(@"Number of objects in array:%d",[array1 count]);
    CFALog(@"%@ \n",array1);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Adding Two Arrays into One");
    array2 = [NSMutableArray arrayWithCapacity:0];
    [array2 addObjectsFromArray:array0];
    [array2 addObjectsFromArray:array1];
    CFALog(@"Number of objects in array:%d",[array2 count]);
    CFALog(@"%@ \n",array2);
    
    CFALog(@"------------------------------------------------");
    CFALog(@"Removing Objects From Another Array");
    [array2 removeObjectsInArray:array0];
    CFALog(@"Number of objects in array:%d",[array2 count]);
    CFALog(@"%@ \n",array2);
    
    
    currentString = [CFAString stringWithString:@"Final Array"];
    p.y -= 32;
    [currentString drawAtPoint:p];
    
    currentString = [CFAString stringWithFormat:@"%@",array2];
    p.y -= 132;
    [currentString drawAtPoint:p];
    
    /*
     There are a few more advanced functions for manipulating NSMutableArray objects
     Have a look at Apple's documentation for these methods
     */
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

