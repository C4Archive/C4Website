  g<entry>
   <title>Modulus Float</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/ModulusFloat/</id>
   <updated>2011-08-19T04:16:36+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/ModulusFloat/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/98.ModulusFloat/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int num;
double c;

@implementation CFACanvas 

-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [CFAShape fill:1.0f];
    [CFAShape stroke:1.0f];
    num = 20;
    c = 0;
}

-(void)draw {
    [self background:0];
    c += .09;
    for(double i = 1; i < self.canvasHeight/num; i++){
        //cocoa cannot perform modulus division ( % ) with floats
        //e.g. 100%(5.2) won't work
        //since it works with only ints, so use fmodf() if you need
        //the following equation translates to (i%c)*i*i
        float x = fmodf(c,(float)i)*i*i; 
        [CFAShape lineFromX:0 Y:i*num toX:x Y:i*num];
        [CFAShape rectWithXPos:x yPos:i*num-num/2 width:8 andHeight:num];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Increment Decrement</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/IncrementDecrement/</id>
   <updated>2011-08-19T04:16:49+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/IncrementDecrement/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/97.IncrementDecrement/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float a, b;
float colorIncrement;
BOOL direction;

@implementation CFACanvas 

-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    a = 0;
    b = self.canvasWidth;
    colorIncrement = 1.0f/(float)self.canvasWidth;
    direction = YES;
}

-(void)draw {
    a++;
    if(a > self.canvasWidth){
        a = 0;
        direction = !direction;
    }
    
    if(direction){
        [CFAShape stroke:a*colorIncrement];
    } else {
        [CFAShape stroke:1.0f-a*colorIncrement];
    }
    [CFAShape lineFromX:a Y:0 toX:a Y:self.canvasHeight/2];
    
    b--;
    if (b < 0) {
        b = self.canvasWidth;
    }
    if (direction) {
        [CFAShape stroke:1.0f-b*colorIncrement];
    } else {
        [CFAShape stroke:b*colorIncrement];
    }
    [CFAShape lineFromX:b Y:self.canvasHeight/2+1 toX:b Y:self.canvasHeight];
}

@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Distance</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/Distance/</id>
   <updated>2011-08-19T04:16:43+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/Distance/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/96.Distance/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int thin = 8;
int thick = 36;
float x1 = 134.0f;
float x2 = 44.0f;
float x3 = 58.0f;
float x4 = 120.0f;

@implementation CFACanvas 
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [CFAShape noStroke];
}

-(void)draw {
    [self background:0];
    
    float mx = mousePos.x * 0.4 - self.canvasWidth/5;
    
    [CFAShape fill:0.4f];
    [CFAShape rectWithXPos:x2 yPos:0 width:thick andHeight:self.canvasHeight/2];
    [CFAShape fill:0.8f];
    [CFAShape rectWithXPos:x1 yPos:0 width:thick andHeight:self.canvasHeight/2];
    [CFAShape fill:0.4f];
    [CFAShape rectWithXPos:x4 yPos:self.canvasHeight/2 width:thin andHeight:self.canvasHeight/2];
    [CFAShape fill:0.8f];
    [CFAShape rectWithXPos:x3 yPos:self.canvasHeight/2 width:thick andHeight:self.canvasHeight/2];
    
    x1 += mx/16;
    x2 += mx/64;
    x3 -= mx/16;
    x4 -= mx/64;
    
    if(x1 < -thick) x1 = self.canvasWidth;
    if(x1 > self.canvasWidth) x1 = -thick;
    if(x2 < -thin) x2 = self.canvasWidth;
    if(x2 > self.canvasWidth) x2 = -thin;
    if(x3 < -thick) x3 = self.canvasWidth;
    if(x3 > self.canvasWidth) x3 = -thick;
    if(x4 < -thin) x4 = self.canvasWidth;
    if(x4 > self.canvasWidth) x4 = -thin;
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Random Multiplication</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/RandomMultiplication/</id>
   <updated>2011-08-19T04:17:04+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/RandomMultiplication/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/95.RandomMultiplication/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

CFAString *a, *b, *equation;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self drawStyle:EVENTBASED];
    [CFAString fill:1.0f];
}

-(void)draw {
    [self background:0];
    int A = [CFAMath randomInt:1000];
    int B = [CFAMath randomInt:1000];
    
    a = [CFAString stringWithFormat:@"A = %d",A];
    b = [CFAString stringWithFormat:@"B = %d",B];
    equation = [CFAString stringWithFormat:@"A * B = %d", A*B];
    
    [a drawAtPoint:NSMakePoint(50, self.canvasHeight*3/4)];
    [b drawAtPoint:NSMakePoint(50, self.canvasHeight/2)];
    [equation drawAtPoint:NSMakePoint(50, self.canvasHeight/4)];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
