    g<entry>
   <title>Integers and Floats</title>
   <id>tag:cocoaforartists.org,2011:/examples/Data/IntegersFloats/</id>
   <updated>2011-08-19T04:17:41+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Data/IntegersFloats/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/996.Data/100.IntegersFloats/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
int a = 0;
float b = 0.0f;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [CFAShape strokeWidth:4];
    [CFAShape stroke:1.0f];
}
-(void)draw {
    [self background:0.2f alpha:0.1f];
    a = a+1.5;
    b = b+1.5;
    [CFAShape lineFromX:a Y:0 toX:a Y:self.canvasHeight/2];
    [CFAShape lineFromX:b Y:self.canvasHeight/2 toX:b Y:self.canvasHeight];
    if (a > self.canvasWidth) {
        a = 0;
    }
    if (b > self.canvasWidth) {
        b = 0;
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>True False</title>
   <id>tag:cocoaforartists.org,2011:/examples/Data/TrueFalse/</id>
   <updated>2011-08-19T04:17:37+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Data/TrueFalse/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/996.Data/99.TrueFalse/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

BOOL x = NO;

@implementation CFACanvas
-(void)setup {
    [self flipCoordinates];
    [self windowWidth:400 andHeight:320];
}
-(void)draw {
    for(int i = 1; i < self.canvasWidth; i+=4){
        if (i < self.canvasWidth/2) {
            x = YES;
        } else {
            x = NO;
        }
        if(x) {
            [CFAShape stroke:1.0f];
            [CFAShape lineFromX:i Y:1 toX:i Y:self.canvasHeight-1];
        }
        if(!x) {
            [CFAShape stroke:0.5f];
            [CFAShape lineFromX:self.canvasWidth/2 Y:i toX:self.canvasWidth-1 Y:i];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Datatype Conversion</title>
   <id>tag:cocoaforartists.org,2011:/examples/Data/DatatypeConversion/</id>
   <updated>2011-08-19T04:17:20+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Data/DatatypeConversion/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/996.Data/98.DatatypeConversion/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
char c;
float f;
int i;
Byte b;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0.9f];
    [CFAShape noStroke];
    c = 'w';
    //when you wish to convert between data types
    //you should alway "cast" them by writing the new type in brackets
    //do this directly before the variable
    f = (float)c;       //change to a character to float type
    i = (int)(f*1.4);   //change a float to an integer
    b = (Byte)(c/2);    //change an integer to a byte value
}
-(void)draw {
    //if you forget to "cast", you can sometimes get away with it...
    //but sometimes the result won't be what you expect
    
    //because the values here will convert to integers larger than 1
    //we need to convert them down to float values (between 0 and 1) to use for colors
    [CFAShape fillRed:FLOAT_FROM_255('A') green:0 blue:0];
    [CFAShape rectWithXPos:f yPos:1 width:'A' andHeight:'A'];
    [CFAShape fillRed:FLOAT_FROM_255('a') green:0 blue:0];
    [CFAShape rectWithXPos:i yPos:67 width:c andHeight:c];
    [CFAShape fillRed:FLOAT_FROM_255('z') green:0 blue:0];
    [CFAShape rectWithXPos:b yPos:134 width:b andHeight:-1*'z'];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Variable Math</title>
   <id>tag:cocoaforartists.org,2011:/examples/Data/VariableMath/</id>
   <updated>2011-08-19T04:17:24+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Data/VariableMath/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/996.Data/97.VariableMath/01 14-08-30.png" alt="01 14 08 30."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [CFAShape stroke:1.0f];
}
-(void)draw {
    int a = 40;
    int b = 80;
    int c = a*8;
    int d = a*9;
    int e = b-a;
    int f = b*2;
    int g = f+e;
    
    [CFAShape lineFromX:a Y:f toX:b Y:g];
    [CFAShape lineFromX:b Y:e toX:d Y:c];
    [CFAShape lineFromX:a Y:e toX:d-e Y:c];
    
    [CFAShape strokeRed:0.5 green:0.5 blue:1.0];
    [CFAShape lineFromX:b Y:e toX:b Y:g];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Variable Scope</title>
   <id>tag:cocoaforartists.org,2011:/examples/Data/VariableScope/</id>
   <updated>2011-08-19T04:17:34+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Data/VariableScope/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/996.Data/96.VariableScope/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas (private)
-(void)drawAnotherLine;
-(void)drawYetAnotherLine;
@end

int a = 20;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0.36f];
    [CFAShape strokeWidth:1];
    [CFAShape stroke:1.0f];
}
-(void)draw {
    [CFAShape lineFromX:a Y:0 toX:a Y:self.canvasHeight];
    for(int a = 60; a < 160; a += 4) {
        [CFAShape lineFromX:a Y:0 toX:a Y:self.canvasHeight];
    }
    int a = 200;
    [CFAShape strokeRed:0.0f green:0.0f blue:1.0f];
    [CFAShape lineFromX:a Y:0 toX:a Y:self.canvasHeight];
    [self drawAnotherLine];
    [self drawYetAnotherLine];
}
-(void)drawAnotherLine {
    int a = 385;
    [CFAShape strokeRed:1.0f green:0.0f blue:0.0f];
    [CFAShape lineFromX:a Y:0 toX:a Y:self.canvasHeight];
}
-(void)drawYetAnotherLine {
    [CFAShape strokeRed:0.0f green:1.0f blue:0.0f];
    [CFAShape lineFromX:a+2 Y:0 toX:a+2 Y:self.canvasHeight];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Variable Ellipses</title>
   <id>tag:cocoaforartists.org,2011:/examples/Data/VariableEllipses/</id>
   <updated>2011-08-19T04:17:31+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Data/VariableEllipses/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/996.Data/95.VariableEllipses/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int size = 80;
int direction = -1;
int step = 1;

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self drawStyle:ANIMATED];
}
-(void)draw {
    if(mouseIsPressed){
        [CFAShape stroke:1.0f];
        [CFAShape fill:0.0f];
    } else {
        [CFAShape stroke:0.0f];
        [CFAShape fillRed:mousePos.x/self.canvasWidth green:mousePos.y/self.canvasWidth blue:0.0f];
    }
    
    [CFAShape ellipseWithXPos:mousePos.x yPos:mousePos.y width:size andHeight:size];
    size = size + direction*step;
    if (size == 10 || size == 90) {
        direction = direction * -1;
    }
}

@end]]></script>

          
      </div>
   </content>
</entry>
  

