  g<entry>
   <title>Custom Methods</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/CustomMethods/</id>
   <updated>2011-08-19T04:19:27+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/CustomMethods/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/89.CustomMethods/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas () {}
-(void)drawTargetAtX:(float)x Y:(float)y withSize:(float)size andNumber:(float)num;
@end

@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:51];
    [CFAShape noStroke];
}

-(void)draw {
    [self drawTargetAtX:252 Y:220 withSize:100 andNumber:3];
    [self drawTargetAtX:200 Y:134 withSize:200 andNumber:10];
    [self drawTargetAtX:80 Y:244 withSize:80 andNumber:5];
}

-(void)drawTargetAtX:(float)x Y:(float)y withSize:(float)size andNumber:(float)num {
    float greyValues = 1.0f/num;
    float steps = size/num;
    for(int i = 0; i < num; i ++){
        [CFAShape fill:i*greyValues];
        [CFAShape ellipseWithXPos:x 
                             yPos:y 
                            width:size-i*steps 
                        andHeight:size-i*steps];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Recursive Circle</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/RecursiveCircle/</id>
   <updated>2011-08-19T04:19:51+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/RecursiveCircle/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/88.RecursiveCircle/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas () {}
-(void)recursiveCirclesAtX:(int)x withRadius:(int)radius andLevels:(int)levels;
@end

@implementation CFACanvas

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0.2f];
    [CFAShape noStroke];
}

-(void)draw {
    [self recursiveCirclesAtX:self.canvasWidth*3/4 withRadius:500 andLevels:6];
}

-(void)recursiveCirclesAtX:(int)x withRadius:(int)radius andLevels:(int)levels {
    float tt = 0.5 * levels / 4.0;
    [CFAShape fill:tt];
    [CFAShape ellipseWithXPos:x yPos:self.canvasHeight/3 width:radius*2 andHeight:radius*2];
    
    if(levels > 1) {
        levels = levels - 1;
        [self recursiveCirclesAtX:x-radius/2 withRadius:radius/2 andLevels:levels];
        [self recursiveCirclesAtX:x+radius/2 withRadius:radius/2 andLevels:levels];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Recursive Random</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/RecursiveCircleRandom/</id>
   <updated>2011-08-19T04:19:19+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/RecursiveCircleRandom/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/87.RecursiveCircleRandom/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas () {}
-(void)recursiveCircle:(NSPoint)p radius:(int)radius level:(int)level;
@end

@implementation CFACanvas

-(void)setup { 
    [self windowWidth:400 andHeight:320]; 
    [CFAShape noStroke]; 
    [self drawStyle:EVENTBASED]; 
} 
-(void)draw { 
    [self background:1.0f]; 
    [self recursiveCircle:NSMakePoint(200, 200) radius:160 level:8]; 
} 

-(void)recursiveCircle:(NSPoint)p radius:(int)radius level:(int)level { 
    float greyValue = 0.5 * level/6.0; 
    [CFAShape fill:greyValue alpha:0.5]; 
    [CFAShape circleAt:p radius:radius*2]; if (level > 1) {
        level--;
        int num = [CFAMath randomIntBetweenA:2 andB:5];
        for(int i = 0; i < num; i++) {
            float a = [CFAMath randomIntBetweenA:0 andB:TWO_PI*100]/100.0f;
            float newX = p.x + [CFAMath cos:a] * 12.0 * level;
            float newY = p.y + [CFAMath sin:a] * 12.0 * level;
            [self recursiveCircle:NSMakePoint(newX, newY) radius:radius/2 level:level];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

  g<entry>
   <title>Advanced</title>
