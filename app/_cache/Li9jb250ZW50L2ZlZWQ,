  g<entry>
   <title>Pointilism</title>
   <id>tag:cocoaforartists.org,2011:/examples/Image/Pointilism/</id>
   <updated>2011-08-19T04:22:42+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Image/Pointilism/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/993.Image/94.Pointilism/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

CFAImage *myImage;

@implementation CFACanvas
-(void)setup {
    //Usually you should set up your canvas first
    //But, since we use the image dimensions to set the canvas size, we should create the image first...
    myImage = [CFAImage imageName:@"C4Sky.png"];
    
    [self windowWidth:myImage.imageWidth andHeight:myImage.imageHeight];
    [self drawStyle:DISPLAYRATE];
    [self background:1.0f];
    
    [myImage loadPixelData];
    
    [CFAShape strokeWidth:1];
    [CFAShape stroke:1];
}

-(void)draw {
    int x = [CFAMath randomInt:self.canvasWidth];
    int y = [CFAMath randomInt:self.canvasHeight];
    CFAColor *color = [myImage colorAtX:x andY:y];
    float pointilize = [CFAMath map:mousePos.x fromMin:0 max:self.canvasWidth toMin:2 max:18];
    [CFAShape fillColor:color];
    [CFAShape circleAt:NSMakePoint(x, y) radius:pointilize];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Pixelism</title>
   <id>tag:cocoaforartists.org,2011:/examples/Image/Pixelism/</id>
   <updated>2011-08-19T04:21:38+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Image/Pixelism/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/993.Image/93.Pixelism/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

NSInteger blockWidth;
NSInteger blockHeight;
NSInteger block;
NSInteger stripeWidth;
NSInteger left;
NSInteger top;

CFAImage *image;

@implementation CFACanvas

-(void)setup {
    //Usually you should set up your canvas first
    //But, since we use the image dimensions to set the canvas size, we should create the image first...
    image = [CFAImage imageName:@"C4Sky" andType:@"png"];
    
    [self windowWidth:image.imageWidth andHeight:image.imageHeight];
    [self background:1.0f];
    
    //stripe = width of 1 color
    stripeWidth = 1;
    //block = 3 * 3 colors (keeping things square)
    blockHeight = stripeWidth*3;
    blockWidth = blockHeight;
    
    [image loadPixelData];
    [CFAShape fill];
    [CFAShape noStroke];
}

-(void)draw {
    [self background:0];
    //iterate through the y coordinates of our image
    for(int y = 0; y <= self.canvasHeight; y+=blockHeight) {
        //iterate through the x coordinates of our image
        for(int x = 0; x <= self.canvasWidth; x+=blockWidth) {            
            //grab the color from our image at (x,y)
            CFAColor *pixelColor = [image colorAtX:x andY:y];
            
            //create a block of colors
            [CFAShape fillRed:[[pixelColor nsColor] redComponent] green:0 blue:0];
            [CFAShape rectWithXPos:x+stripeWidth*0 
                              yPos:y 
                             width:stripeWidth 
                         andHeight:blockHeight];
            
            [CFAShape fillRed:0 green:[[pixelColor nsColor] greenComponent] blue:0];
            [CFAShape rectWithXPos:x+stripeWidth*1 
                              yPos:y 
                             width:stripeWidth 
                         andHeight:blockHeight];
            
            [CFAShape fillRed:0 green:0 blue:[[pixelColor nsColor] blueComponent]];
            [CFAShape rectWithXPos:x+stripeWidth*2
                              yPos:y 
                             width:stripeWidth 
                         andHeight:blockHeight];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
