    g<entry>
   <title>Every Letter Every Font</title>
   <id>tag:cocoaforartists.org,2011:/examples/Advanced/EveryLetter/</id>
   <updated>2011-08-19T04:21:03+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Advanced/EveryLetter/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1001.Advanced/99.EveryLetter/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int fontCount, currentFont, characterIndex, size;
NSPoint point;
NSSize backgroundRectSize;
NSArray *fontArray, *characterArray;
CFAString *description, *character;

@implementation CFACanvas

-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:[self screenWidth]-30 andHeight:[self screenHeight]-60];
    [self background:1];
    fontArray = [CFAFont availableFonts];
    fontCount = [fontArray count];
    currentFont = 0;
    
    size = 18;
    characterArray = [[[NSArray alloc] initWithArray:
                       [[CFAString stringWithString:
                         @"a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z"]
                        componentsSeparatedByString:@","]] retain];
    characterIndex = 0;
    character = [[CFAString stringWithString:[characterArray objectAtIndex:characterIndex]] retain];
    
    [CFAShape noStroke];
    [description font:[CFAFont fontWithFont:[CFAFont userFontOfSize:12]]];
    
    point = NSMakePoint(size, size*2);
}

-(void)draw {
    [CFAShape fill:0.2];
    [CFAShape rectWithXPos:0 yPos:0 width:self.canvasWidth andHeight:26];
    
    description = [CFAString stringWithFormat:@"current font: %@ (%d/%d)",
                   [fontArray objectAtIndex:currentFont], 
                   currentFont, 
                   fontCount];
    [description fill:1];
    [description drawAtPoint:NSMakePoint(10, 6)];
    
    [CFAShape fill:1];
    [CFAShape rectAt:point size:backgroundRectSize];
    [character font:[CFAFont fontWithName:[fontArray objectAtIndex:currentFont] size:size]];
    [character drawAtPoint:point];
    
    point.x += [character size].width;
    if (point.x + [character size].width >= self.canvasWidth-size) {
        point.x = size;
        point.y += size;
        if(point.y >= self.canvasHeight-size) {
            point.y = size*2;
        }
    }
    
    currentFont++;
    
    if(currentFont >= fontCount) {
        currentFont = 0;
        characterIndex++;
        if(characterIndex == [characterArray count]) characterIndex = 0;
        character = [[CFAString stringWithString:[characterArray objectAtIndex:characterIndex]] retain];
        [character fillRed:FLOAT_FROM_255([CFAMath randomInt:255]) 
                     green:FLOAT_FROM_255([CFAMath randomInt:255]) 
                      blue:FLOAT_FROM_255([CFAMath randomInt:255])];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Storing Input Advanced</title>
   <id>tag:cocoaforartists.org,2011:/examples/Advanced/StoringInput/</id>
   <updated>2011-08-19T04:20:55+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Advanced/StoringInput/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1001.Advanced/98.StoringInput/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@interface CFACanvas ()
-(void)updateEasingPoint:(NSPoint *)point usingEaseValue:(float)easeValue;
@end

NSPoint *points;
int pointCount = 200;
float easing = 0.03f;

@implementation CFACanvas

-(void)setup {
    points = (NSPoint *)calloc(pointCount,sizeof(NSPoint));
    
    for(int i = 0; i < pointCount; i++) {
        points[i].x = self.canvasWidth/2;
        points[i].y = self.canvasHeight/2;
    }
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    [CFAShape noStroke];
}

-(void)draw {
    [self background:0.2f];
    for(int i = 0; i < pointCount; i++) {
        [self updateEasingPoint:&#38;points[i] usingEaseValue:easing+(.003*i)];
        [CFAShape fillRed:1-(float)i/(float)pointCount green:0 blue:(float)i/(float)pointCount];
        [CFAShape circleAt:NSMakePoint(points[i].x, points[i].y) radius:30-(i/10)];
    }
}

-(void)updateEasingPoint:(NSPoint *)point usingEaseValue:(float)easeValue{
    float dx = mousePos.x - point->x;
    if([CFAMath absf:dx] > 1){
        point->x += dx * easeValue;
    }
    float dy = mousePos.y - point->y;
    if([CFAMath absf:dy] > 1){
        point->y += dy * easeValue;
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Kandinskyish</title>
   <id>tag:cocoaforartists.org,2011:/examples/Advanced/Kandinskyish/</id>
   <updated>2011-08-19T04:21:08+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Advanced/Kandinskyish/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1001.Advanced/97.Kandinskyish/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:600 andHeight:400];
    [self background:1.0f];
    [CFAShape stroke];
}
-(void)draw {
    [self backgroundRed:0.945 green:0.88 blue:0.72];
    [self drawBackground];
    
    //GRADIENTS
    [CFAShape clearCurrentShape];
    [CFAShape addTriangleFromX:108 Y:208.5 toX:200.5 Y:305 toX:365 Y:161];
    [CFAGradient linearGradientFromPointA:NSMakePoint(255,385)
                                 toPointB:NSMakePoint(205,205)
                              usingColorA:[CFAColor colorWithRed:0.36f green:0.68f blue:0.85f]
                                andColorB:[CFAColor colorWithRed:0.36f green:0.68f blue:0.85f alpha:0.0f]
                                  inShape:[CFAShape currentShape]];
    
    [CFAShape clearCurrentShape];
    [CFAShape addTriangleFromX:255.5 Y:40 toX:359 Y:402.5 toX:470 Y:20.5];
    [CFAGradient linearGradientFromPointA:NSMakePoint(0,self.canvasHeight)
                                 toPointB:NSMakePoint(0,150)
                              usingColorA:[CFAColor colorWithRed:1.0f green:1.0f blue:1.0f]
                                andColorB:[CFAColor colorWithRed:1.0f green:1.0f blue:1.0f alpha:0.0f]
                                  inShape:[CFAShape currentShape]];
    
    [CFAShape noStroke];
    [CFAShape fill];
    //CIRCLES
    [CFAShape fillRed:0.78 green:0.42 blue:0.3];
    [CFAShape circleAt:NSMakePoint(70, 330) radius:75];
    
    [CFAShape fillRed:0.87 green:0.55 blue:0.5 alpha:0.9];
    [CFAShape circleAt:NSMakePoint(70, 330) radius:69];
    
    [CFAShape fillRed:0.68 green:.08 blue:0];
    [CFAShape circleAt:NSMakePoint(118, 268) radius:24];
    
    [CFAShape fill:0];
    [CFAShape circleAt:NSMakePoint(70, 330) radius:60];
    
    [CFAShape fillRed:0.325 green:0.185 blue:0.35];
    [CFAShape circleAt:NSMakePoint(70, 330) radius:29];
    
    [CFAShape fillRed:0.41 green:0.59 blue:0.8 alpha:.8];
    [CFAShape circleAt:NSMakePoint(60, 112) radius:33];
    
    [CFAShape stroke];
    [CFAShape fillRed:0.91 green:0.7 blue:.235];
    [CFAShape circleAt:NSMakePoint(60, 112) radius:21];
    
    [CFAShape noStroke];
    [CFAShape fillRed:0.96 green:0.84 blue:0.37];
    [CFAShape circleAt:NSMakePoint(225.5, 44.5) radius:26];
    
    [CFAShape fillRed:0.92 green:0.65 blue:0.5];
    [CFAShape circleAt:NSMakePoint(224, 44.5) radius:18.5];
    
    [CFAShape strokeWidth:0.5];
    [CFAShape stroke];
