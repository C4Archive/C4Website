  g<entry>
   <title>Logic Operators</title>
   <id>tag:cocoaforartists.org,2011:/examples/Control/Logic/</id>
   <updated>2011-08-19T04:23:08+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Control/Logic/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/997.Control/98.Logic/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

BOOL op = NO;

@implementation CFACanvas
-(void)setup {
[self windowWidth:400 andHeight:320];
}
-(void)draw {
for(int i = 5; i <= self.canvasHeight-5; i+=5){
[CFAShape stroke];
[CFAShape stroke:0.22f];
if((i > 70) &#38;&#38; (i < 200)) {
[CFAShape lineFromX:5 Y:i toX:145 Y:i];
op = NO;
}
[CFAShape stroke:0.44f];
if (i <= 70 || i >= 200) {
[CFAShape lineFromX:155 Y:i toX:295 Y:i];
op = YES;
}
if(op) {
[CFAShape fillRed:1.0f green:0.2f blue:0.2f]; //because we use fill to define point color
[CFAShape pointAtX:self.canvasWidth/2 Y:i];
}
if(!op) {
[CFAShape fillRed:0.2f green:0.4f blue:1.0f]; //because we use fill to define point color
[CFAShape pointAtX:self.canvasWidth/4 Y:i];
}
}
}
@end
]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Iteration</title>
   <id>tag:cocoaforartists.org,2011:/examples/Control/Iteration/</id>
   <updated>2011-08-19T04:23:11+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Control/Iteration/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/997.Control/97.Iteration/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
int k;
int xpos1 = 100;
int xpos2 = 118;
int count = 0;
int timey = 0;
int num = 12;

@implementation CFACanvas
-(void)setup {
    [self flipCoordinates];
    [self windowWidth:400 andHeight:320];
    [self background:0.4f];
    [CFAShape noStroke];
}
-(void)draw {
    [CFAShape fill:1.0f];
    k = 65;
    for(int i = 0; i < num/3; i++) {
        [CFAShape rectWithXPos:25 yPos:k width:155 andHeight:10];
        k+=20;
    }
    [CFAShape fill:0.2f];
    k = 100;
    for(int i = 0; i < num; i++) {
        [CFAShape rectWithXPos:160 yPos:k width:30 andHeight:10];
        k+=20;
    }
    k = 15;
    for(int i = 0; i < num; i++) {
        [CFAShape rectWithXPos:225 yPos:k width:30 andHeight:10];
        k+=20;
    }
    [CFAShape fill:0];
    k = 50;
    for(int i = 0; i < num-1; i++) {
        [CFAShape rectWithXPos:36 yPos:k width:20 andHeight:2];
        k+=20;
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Embedded Iteration</title>
   <id>tag:cocoaforartists.org,2011:/examples/Control/EmbeddedIteration/</id>
   <updated>2011-08-19T04:23:13+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Control/EmbeddedIteration/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/997.Control/96.EmbeddedIteration/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
float box_size = 16.0f;
float box_space = 17.0f;
int margin = 12;

@implementation CFACanvas
-(void)setup {
    [self background:0];
    [self windowWidth:400 andHeight:320];
    [CFAShape noStroke];
    [CFAShape fill];
}
-(void)draw {
    for(int i = margin; i <= self.canvasHeight-margin &#38;&#38; box_size > 1; i+=box_space){
        if (box_size > 1) {
            for(int j = margin; j < self.canvasWidth-margin; j+=box_space){
                [CFAShape fill:3*self.canvasHeight/(i*box_space)];
                [CFAShape ellipseWithXPos:j yPos:i width:box_size andHeight:box_size];
            }
        }
        box_size = box_size-.6;
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

