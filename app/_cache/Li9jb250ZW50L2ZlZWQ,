    g<entry>
   <title>Abstract Keyboard 1</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/AbstractKeyboard1/</id>
   <updated>2011-08-19T04:17:58+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/AbstractKeyboard1/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/99.AbstractKeyboard1/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float rectWidth;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:ANIMATED];
    [self windowWidth:400 andHeight:320];
    rectWidth = self.canvasWidth/26.0f;
    [CFAShape noStroke];
    [CFAShape fill:1.0f];
    [CFAShape fill];
}

-(void)draw{    
    [self background:0 alpha:0.015f];
    if (keyIsPressed &#38;&#38; keyChar >= 'A' &#38;&#38; keyChar <= 'z') {
        if (keyChar <= 'Z') {
            int mappedKeyValue = keyChar - 'A';
            [CFAShape rectWithXPos:rectWidth*mappedKeyValue 
                              yPos:self.canvasHeight/2 
                             width:rectWidth
                         andHeight:self.canvasHeight/2];
        } else {
            int mappedKeyValue = keyChar - 'a';
            [CFAShape rectWithXPos:rectWidth*mappedKeyValue 
                              yPos:0 
                             width:rectWidth 
                         andHeight:self.canvasHeight/2];
        }
    }
}
@end
]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Abstract Keyboard 2</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/AbstractKeyboad2/</id>
   <updated>2011-08-19T04:17:45+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/AbstractKeyboad2/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/98.AbstractKeyboad2/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int max_height;
int min_height;
int letter_height;
int letter_width;
int x;
int y;

BOOL newLetter;
int numChars;

@implementation CFACanvas

-(void)setup {
    [self drawStyle:EVENTBASED];
    [self windowWidth:400 andHeight:320];
    [self background:0];
    
    [CFAShape fill];
    [CFAShape noStroke];
    
    max_height = 20;
    min_height = 10;
    letter_height = max_height;
    letter_width = min_height;
    numChars = 26;
}

-(void)draw {
    if (newLetter) {
        unichar character = keyChar;
        if (character >= 'A' &#38;&#38; character <='z') {
            int characterIndex;
            if(character <= 'Z'){
                characterIndex = character - 'A';
                letter_height = max_height;
            } else {
                characterIndex = character - 'a';
                letter_height = min_height;
            }
            
            int fill = [CFAMath constrain:characterIndex*10 min:0 max:255];
            [CFAShape fill:FLOAT_FROM_255(fill)];
        } else {
            [CFAShape fill:0];
            letter_height = min_height;
        }
        
        newLetter = YES;
        
        x += letter_width;
        if (x >= self.canvasWidth) {
            x = 0;
            y += max_height;
            
            if (y >= self.canvasHeight) {
                y = 0;
            }
        }
        
        int y_pos;
        if(letter_height == max_height) {
            y_pos = y;
            [CFAShape rectWithXPos:x 
                              yPos:y_pos 
                             width:letter_width
                         andHeight:letter_height];
        } else {
            y_pos = y + min_height;
            [CFAShape rectWithXPos:x 
                              yPos:y_pos 
                             width:letter_width 
                         andHeight:letter_height];
            
            [CFAShape fill:FLOAT_FROM_255(numChars/2)];
            [CFAShape rectWithXPos:x
                              yPos:y_pos-min_height 
                             width:letter_width 
                         andHeight:letter_height];
        }
        newLetter = NO;
    }
}

-(void)keyPressed {
    newLetter = YES;
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Constant Type</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/ConstantType/</id>
   <updated>2011-08-19T04:18:10+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/ConstantType/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/97.ConstantType/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

CFAString *theString;

@implementation CFACanvas

-(void)setup {
    [self drawStyle:EVENTBASED];
    [self windowWidth:400 andHeight:320];
    [CFAString fill:1];
    theString = [[CFAString stringWithString:@""] retain];
}

-(void)draw {
    [self background:0];
    [theString drawInRect:self.canvasRect];
}

-(void)keyPressed {
    if(keyChar == RETURN) theString = [[CFAString stringWithString:@""] retain];
    else theString = [[theString stringByAppendingFormat:@"%c",keyChar] retain];
}
@end
]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Mouse 1D</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/Mouse1D/</id>
   <updated>2011-08-19T04:17:48+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/Mouse1D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/96.Mouse1D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

@interface CFACanvas () {}
-(void)update:(int)x;
@end

int gx = 15;
int gy = 35;
float leftColor = 0.0f;
float rightColor = 0.0f;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:EVENTBASED];
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [CFAShape noStroke];
    [CFAShape ellipseMode:CORNER];
}

-(void)draw {
    [self background:0];
    [self update:mousePos.x];
    [CFAShape fillRed:(leftColor + 0.6) 
                green:(leftColor+0.4) 
                 blue:0];
    [CFAShape ellipseWithXPos:self.canvasWidth/4-gx 
                         yPos:self.canvasWidth/2-gx-40 
                        width:gx*2 
                    andHeight:gx*2];
    [CFAShape fillRed:(rightColor + 0.4) 
                green:(rightColor+0.2) 
                 blue:0];
    [CFAShape ellipseWithXPos:self.canvasWidth/1.33-gy 
                         yPos:self.canvasWidth/2-gy-40 
                        width:gy*2 
                    andHeight:gy*2];
}

-(void)update:(int)x {
    leftColor = -0.002 * x/3 + 0.06;
    rightColor =  0.002 * x/3 + 0.06;
    
    gx = x/2;
    gy = 200-x/2;
    
    if (gx < 10) {
        gx = 10;
    } else if (gx > 190) {
        gx = 190;
    }
    
    if (gy > 190) {
        gy = 190;
    } else if (gy < 10) {
        gy = 10;
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Mouse 2D</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/Mouse2D/</id>
   <updated>2011-08-19T04:18:01+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/Mouse2D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
