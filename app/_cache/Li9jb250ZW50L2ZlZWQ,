  g<entry>
   <title>Shape Modes</title>
   <id>tag:cocoaforartists.org,2011:/examples/Shapes/ShapeModes/</id>
   <updated>2011-08-19T04:18:58+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Shapes/ShapeModes/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/984.Shapes/98.ShapeModes/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
}
-(void)draw {
    /*
     default rectMode is CORNER
     default ellipseMode is CENTER
     */

    [CFAShape rectWithXPos:self.canvasWidth/4-20
                      yPos:self.canvasHeight/2
                     width:80 
                 andHeight:80];
    [CFAShape ellipseWithXPos:self.canvasWidth*3/4-20 
                         yPos:self.canvasHeight/2 
                        width:80 
                    andHeight:80];
    
    [CFAShape fill:0];

    [CFAShape rectMode:CENTER];
    [CFAShape ellipseMode:CORNER];
    [CFAShape rectWithXPos:self.canvasWidth*1/4-20 
                      yPos:self.canvasHeight/2 
                     width:80
                 andHeight:80];
    [CFAShape ellipseWithXPos:self.canvasWidth*3/4-20 
                         yPos:self.canvasHeight/2 
                        width:80 
                    andHeight:80];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Points and Lines</title>
   <id>tag:cocoaforartists.org,2011:/examples/Shapes/PointsLines/</id>
   <updated>2011-08-19T04:18:49+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Shapes/PointsLines/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/984.Shapes/97.PointsLines/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self background:0.0f];
}

-(void)draw {
    int d = 66;
    int p1 = d;
    int p2 = p1+d;
    int p3 = p2+d;
    int p4 = p3+d;
    [CFAShape stroke:0.6f];
    // Draw gray box
    [CFATransform begin];
    [CFATransform translateByX:33 andY:0];
    [CFATransform concat];
    [CFAShape lineFromX:p3 Y:p3 toX:p2 Y:p3];
    [CFAShape lineFromX:p2 Y:p3 toX:p2 Y:p2];
    [CFAShape lineFromX:p2 Y:p2 toX:p3 Y:p2];
    [CFAShape lineFromX:p3 Y:p2 toX:p3 Y:p3];
    
    [CFAShape fill:1.0f];
    [CFAShape pointAtX:p1 Y:p1];
    [CFAShape pointAtX:p1 Y:p3];
    [CFAShape pointAtX:p2 Y:p4];
    [CFAShape pointAtX:p3 Y:p1];
    [CFAShape pointAtX:p4 Y:p2];
    [CFAShape pointAtX:p4 Y:p4];
    [CFATransform end];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Vertex Shapes</title>
   <id>tag:cocoaforartists.org,2011:/examples/Shapes/VertexShapes/</id>
   <updated>2011-08-19T04:18:55+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Shapes/VertexShapes/" />
