  g<entry>
   <title>C4 Particles</title>
   <id>tag:cocoaforartists.org,2011:/examples/C4/C4Particles/</id>
   <updated>2011-08-19T04:14:44+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/C4/C4Particles/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/999.C4/87.C4Particles/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
NSPoint points[371];

#import <Cocoa/Cocoa.h>

NSMutableArray *particleArray;
NSPoint startLocation;

////////////////////////////////////////////////////////////////
// The Particle object
////////////////////////////////////////////////////////////////

@interface Particle : CFAObject {
@private
    CFAVector *location, *velocity, *acceleration, *prevLocation;
    NSPoint target;
    float topSpeed;
    NSInteger targetIndex;
    CGFloat distanceToTarget;
}

-(void)setTarget:(NSNotification *)mouseMovedNotification;
-(void)setShapeTarget;
-(void)draw;
@end

@implementation Particle
-(id)init {
    if(!(self = [super init])) {
        return nil;
    }
    
    location = [CFAVector vectorWithX:[CFAMath randomInt:400]
                                    Y:[CFAMath randomInt:320]
                                    Z:0];
    
    prevLocation = [CFAVector vectorWithX:0 Y:0 Z:0];
    
    velocity = [CFAVector vectorWithX:0 Y:0 Z:0];
    topSpeed = [CFAMath randomInt:4]+2.5f;
    
    [self updateAutomaticallyUsingFrames:1];
    
    [self setShapeTarget];
    return self;
}

-(void)setTarget:(NSNotification *)mouseMovedNotification {
    target = [[[mouseMovedNotification userInfo] objectForKey:@"mouseEvent"] locationInWindow];
}

-(void)setShapeTarget {
    targetIndex = [CFAMath randomInt:371];
    target = points[targetIndex];
    [location setX:startLocation.x Y:startLocation.y Z:location.z];
    distanceToTarget = [CFAVector distanceBetweenA:target andB:[location point2D]];
}

-(void)update {
    distanceToTarget = [CFAVector distanceBetweenA:target andB:[location point2D]];
    
    if(distanceToTarget <= 5.0f){
        [self setShapeTarget];
    }
    
    [prevLocation setX:location.x Y:location.y Z:location.z];
    
    CFAVector *mouse = [CFAVector vectorWithX:target.x Y:target.y Z:0];
    CFAVector *dir = mouse;
    [dir subtract:location];
    [dir normalize];
    [dir multiplyScalar:-1.15*topSpeed];
    
    [acceleration release];
    acceleration = dir;
    
    [velocity add:acceleration];
    [velocity limit:topSpeed];
    
    [location add:velocity];
}

-(void)draw {
    [CFAShape noStroke];
    if(targetIndex <= 152) {
        [CFAShape fillRed:1.0f green:0.2f blue:0.2f alpha:1.0-distanceToTarget/20.0f];
    } else if (targetIndex <= 335){
        [CFAShape fillRed:0.2f green:0.4f blue:1.0f alpha:1.0-distanceToTarget/20.0f];
    }
    else {
        [CFAShape fill:0.3f alpha:1.0-distanceToTarget/20.0f];
    }
    //[CFAShape pointAt:[location point2D]];
    
    [CFAShape ellipseWithXPos:location.x yPos:location.y width:3 andHeight:3];
}
@end

@implementation CFACanvas

-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    startLocation = NSMakePoint(180, 160);
    
    //C
    points[0].x = 80.00; points[0].y = 80.00;
    points[1].x = 80.00; points[1].y = 85.00;
    points[2].x = 80.00; points[2].y = 90.00;
    points[3].x = 80.00; points[3].y = 95.00;
    points[4].x = 80.00; points[4].y = 100.00;
    points[5].x = 80.00; points[5].y = 105.00;
    points[6].x = 80.00; points[6].y = 110.00;
    points[7].x = 80.00; points[7].y = 115.00;
    points[8].x = 80.00; points[8].y = 120.00;
    points[9].x = 80.00; points[9].y = 125.00;
    points[10].x = 80.00; points[10].y = 130.00;
    points[11].x = 80.00; points[11].y = 135.00;
    points[12].x = 80.00; points[12].y = 140.00;
    points[13].x = 80.00; points[13].y = 145.00;
    points[14].x = 80.00; points[14].y = 150.00;
    points[15].x = 80.00; points[15].y = 155.00;
    points[16].x = 80.00; points[16].y = 160.00;
    points[17].x = 80.00; points[17].y = 165.00;
    points[18].x = 80.00; points[18].y = 170.00;
