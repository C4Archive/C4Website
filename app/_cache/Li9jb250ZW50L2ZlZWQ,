   <title>Noise 1D</title>
   <id>tag:cocoaforartists.org,2011:/examples/Noise/Noise1D/</id>
   <updated>2011-08-19T04:21:15+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Noise/Noise1D/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/989.Noise/99.Noise1D/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float xOff;
float xIncrement;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:DISPLAYRATE];
    [self windowWidth:400 andHeight:320];
    xOff = 0.0f;
    xIncrement = 0.01f;
    [CFAShape fill:0.8];
    [CFAShape noStroke];
}

-(void)draw {
    [self background:0 alpha:0.1];
    float n = [CFANoise noiseX:xOff]*self.canvasWidth;
    xOff += xIncrement;
    [CFAShape ellipseWithXPos:n+self.canvasWidth/2 yPos:self.canvasHeight/2 width:16 andHeight:16];
}
@end]]></script>

          
