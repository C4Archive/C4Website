    g<entry>
   <title>Operator Precedence</title>
   <id>tag:cocoaforartists.org,2011:/examples/Math/OperatorPrecedence/</id>
   <updated>2011-08-19T04:16:46+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Math/OperatorPrecedence/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/991.Math/99.OperatorPrecedence/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float a, b;
float colorIncrement;
BOOL direction;

@implementation CFACanvas 
-(void)setup {
    [self windowWidth:400 andHeight:320];
    [CFAShape noFill];
}

-(void)draw {
    [self background:0.2f];
    [CFAShape stroke:1.0f];
    for(int i = 0; i < self.canvasWidth-20; i+=4){
        if (i > 30 + 70) {
            [CFAShape lineFromX:i Y:0 toX:i Y:50];
        }
    }
    
    [CFAShape rectWithXPos:4+2*8 yPos:52 width:90 andHeight:48];
    [CFAShape rectWithXPos:(4+2)*8 yPos:100 width:90 andHeight:49];
    
    for(int i = 0; i < self.canvasWidth; i += 4){
        if (i > 20 &#38;&#38; i < 50 || i > 100 &#38;&#38; i < self.canvasWidth-20) {
            [CFAShape lineFromX:i Y:151 toX:i Y:self.canvasHeight-1];
        }
    }
}
@end]]></script>

          
      </div>
   </content>
