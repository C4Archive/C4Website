   <title>C4 Points</title>
   <id>tag:cocoaforartists.org,2011:/examples/C4/C4Points/</id>
   <updated>2011-08-19T04:14:49+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/C4/C4Points/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/999.C4/99.C4Points/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

@interface CFACanvas () {} 
-(void)drawC;
-(void)draw4;
-(void)drawSquare;
-(void)setupC4Points;
@end

@implementation CFACanvas

NSPoint points[371];
NSInteger totalPoints;

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self setupC4Points];
}

-(void)draw {
    [self drawC];
    [self draw4];
    [self drawSquare];
}

//Draws a red C, using only points
-(void)drawC {
    [CFAShape fillRed:1.0f green:0.2f blue:0.2f];
    for(int i = 0; i < 152; i++) {
        [CFAShape pointAt:points[i]];
    }
    
}

//Draws a blue 4, using only points
-(void)draw4 {
    [CFAShape fillRed:0.2f green:0.4f blue:1.0f];
    for(int i = 152; i < 335; i++) {
        [CFAShape pointAt:points[i]];
    }
}

//Draws a grey square, using only points
-(void)drawSquare {
    [CFAShape fillRed:0.3f green:0.3f blue:0.3f];
    for(int i = 335; i < 371; i++) {
        [CFAShape pointAt:points[i]];
    }
}

-(void)setupC4Points {
    //C
    points[0].x = 80.00; points[0].y = 80.00;
