    g<entry>
   <title>Width and Height</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/WidthHeight/</id>
   <updated>2011-08-19T04:19:56+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/WidthHeight/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/99.WidthHeight/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

@implementation CFACanvas

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [CFAString fill:1.0f];
}

-(void)draw {
    [[CFAString stringWithFormat:
      @"The canvas dimensions are: %4.2f x %4.2f", self.canvasWidth, self.canvasHeight] 
     drawAtPoint:NSMakePoint(20, self.centrePos.y)];
}

@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Singleframe</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/Singleframe/</id>
   <updated>2011-08-19T04:19:37+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/Singleframe/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/98.Singleframe/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas

float y;

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [CFAString fill:1.0f];
    [CFAShape strokeRed:1.0f green:0.2f blue:0.2f];
    y = self.centrePos.y;
    
}

-(void)draw {
    [CFAShape lineFromX:0 Y:y toX:self.canvasWidth Y:y];
    y++;
    if(y < self.canvasHeight) y = 0;
    
    CFALog(@"The default draw style is SINGLEFRAME");
    CFALog(@"This sketch should send only 1 message");
    
    [[CFAString stringWithString:
      @"The default draw style is SINGLEFRAME\nThis sketch should send only 1 message"] 
     drawAtPoint:NSMakePoint(20, self.canvasHeight/2)];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Redraw</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/Redraw/</id>
   <updated>2011-08-19T04:19:45+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/Redraw/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/97.Redraw/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int y = 160;

@implementation CFACanvas

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self drawStyle:SINGLEFRAME];
    [CFAShape strokeRed:1.0f green:0.2f blue:0.2f];
    [CFAString fill:1.0f];
}

-(void)draw {
    [self background:0];
    y++;
    if (y > self.canvasHeight) y = 0;
    
    [CFAShape lineFromX:0 Y:y toX:self.canvasWidth Y:y];    

    [[CFAString stringWithString:@"Pressing the mouse triggers redraw"]
     drawAtPoint:NSMakePoint(20, self.centrePos.y)];
}

-(void)mousePressed {
    [self redraw];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Eventbased</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/Eventbased/</id>
   <updated>2011-08-19T04:19:41+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/Eventbased/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/96.Eventbased/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas

CFAString *message, *drawcount;

NSInteger drawCount = 0;

CGFloat y;

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self drawStyle:EVENTBASED];
    [CFAString fill:1.0f];
    [CFAShape strokeRed:1.0f green:0.2f blue:0.2f];
    message = [[CFAString stringWithString:
                @"The EVENTBASED style draws only when there is keyboard or mouse interaction"] 
               retain];
    y = self.centerPos.y;
    
}

-(void)draw {
    [self background:0];
    y++;
    if(y > self.canvasHeight) y = 0;
    [CFAShape lineFromX:0 Y:y toX:self.canvasWidth Y:y];
    
    [message drawInRect:NSMakeRect(20, self.centerPos.y, 360, 40)];
    drawcount = [CFAString stringWithFormat:@"drawCount = %d ",drawCount++];
    [drawcount drawAtPoint:NSMakePoint(20, 146)];
    
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Animated</title>
   <id>tag:cocoaforartists.org,2011:/examples/Basics/Animated/</id>
   <updated>2011-08-19T04:20:01+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/Basics/Animated/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/1002.Basics/95.Animated/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"
@implementation CFACanvas

CFAString *message, *drawcount;

NSInteger drawCount = 0;

CGFloat y;

-(void)setup {
    [self windowWidth:400 andHeight:320];
    [self drawStyle:ANIMATED];
    [CFAString fill:1.0f];
    message = [[CFAString stringWithString:
                @"When the draw style is ANIMATED the sketch will fire automatically"] 
               retain];
    [CFAShape strokeRed:1.0f green:0.2f blue:0.2f];
    y = self.centrePos.y;
    
}

-(void)draw {
    [self background:0];
    
    y++;
    if(y > self.canvasHeight) y = 0;
    [CFAShape lineFromX:0 Y:y toX:self.canvasWidth Y:y];
    
    [message drawInRect:NSMakeRect(20, self.centrePos.y, 360, 40)];
    drawcount = [CFAString stringWithFormat:@"drawCount = %d ",drawCount++];
    [drawcount drawAtPoint:NSMakePoint(20, 146)];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
