  g<entry>
   <title>Mouse Methods</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/MouseMethods/</id>
   <updated>2011-08-19T04:18:13+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/MouseMethods/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/94.MouseMethods/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float bx;
float by;
int bs = 30;
BOOL bover = NO;
BOOL locked = NO;
float bdifx = 0.0f;
float bdify = 0.0f;
CFAString *state;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:EVENTBASED];
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [CFAShape noStroke];
    bx = self.canvasWidth/2-bs/2;
    by = self.canvasHeight/2-bs/2;
    [CFAShape strokeWidth:2];
    
    [CFAString fill:1.0f];
    state = [[CFAString stringWithString:@"-"] retain];
}

-(void)draw {
    [self background:0];
    
    if (mousePos.x > bx &#38;&#38; 
        mousePos.x < bx + bs 
        &#38;&#38; mousePos.y > by &#38;&#38; 
        mousePos.y < by+bs) {
        
        bover = YES;
        if (!locked) {
            [CFAShape stroke:255];
            [CFAShape fillRed:1.0f green:0.2f blue:0.2f];
            state = [[CFAString stringWithString:@"hovering"] retain];
        }
    } else {
        [CFAShape stroke:0.6f];
        [CFAShape fillRed:0.3f green:0.3f blue:0.3f];
        state = [[CFAString stringWithString:@"-"] retain];
        bover = NO;
    }
    
    [CFAShape rectWithXPos:bx yPos:by width:bs andHeight:bs];
    [(CFAString *)state drawAtPoint:NSMakePoint(10, 10)];
}

-(void)mousePressed {
    if(bover) {
        locked = YES;
        [CFAShape fillRed:0.2f green:0.4f blue:1.0f];
        state = [[CFAString stringWithString:@"pressed"] retain];
    } else {
        locked = NO;
    }
    bdifx = mousePos.x-bx;
    bdify = mousePos.y-by;
}

-(void)mouseDragged {
    if(locked) {
        state = [[CFAString stringWithString:@"dragging"] retain];
        bx = mousePos.x - bdifx;
        by = mousePos.y - bdify;
    }
}

-(void)mouseReleased {
    locked = NO;
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Mouse Signals</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/MouseSignals/</id>
   <updated>2011-08-19T04:18:04+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/MouseSignals/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/93.MouseSignals/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int totalPixels = 400;
int xvals[400];
int yvals[400];
int bvals[400];
int arrayIndex = 0;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:ANIMATED];
    [self windowWidth:totalPixels andHeight:320];
    [CFAShape strokeWidth:1];
    [CFAShape noStroke];
    [CFAShape stroke:1.0f];
}
-(void)draw {
    [self background:0.5f];
    for(int i = 1; i < self.canvasWidth; i++) {
        xvals[i-1] = xvals[i];
        yvals[i-1] = yvals[i];
        bvals[i-1] = bvals[i];
    }
    
    xvals[totalPixels-1] = mousePos.x;
    yvals[totalPixels-1] = mousePos.y;
    if (mouseIsPressed) {
        bvals[totalPixels-1] = self.canvasHeight*.98;
    } else {
        bvals[totalPixels-1] = 0;
    }
    
    [CFAShape noStroke];
    [CFAShape fill:1.0f];
    [CFAShape rectWithXPos:0 
                      yPos:self.canvasHeight/3 
                     width:self.canvasWidth 
                 andHeight:self.canvasHeight/3+1];
    
    for(int i = 1; i < totalPixels; i++) {
        [CFAShape fill:1.0f];
        [CFAShape pointAtX:i Y:xvals[i]/3+2*self.canvasHeight/3];
        [CFAShape fill:0];
        [CFAShape pointAtX:i Y:yvals[i]/3+self.canvasHeight/3];
        [CFAShape stroke];
        [CFAShape lineFromX:i-1 Y:bvals[i-1]/3 toX:i Y:bvals[i]/3];
    }
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Mouse Coords</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/StoringMouseCoords/</id>
   <updated>2011-08-19T04:17:55+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/StoringMouseCoords/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/92.StoringMouseCoords/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

int xvals[60];
int yvals[60];
int pointCount = 60;

@implementation CFACanvas
-(void)setup {
    [self drawStyle:ANIMATED];
    self.frameRate = 15;
    [self windowWidth:400 andHeight:320];
    [CFAShape noStroke];
    for (int i = 0; i < pointCount; i++) {
        xvals[i] = self.canvasWidth/2;
        yvals[i] = self.canvasHeight/2;
    }
}

-(void)draw {
    [self background:0.3f];
    for(int i = 1; i < pointCount; i++){
        xvals[i-1] = xvals[i];
        yvals[i-1] = yvals[i];
    }
    xvals[pointCount-1] = mousePos.x;
    yvals[pointCount-1] = mousePos.y;
    
    [CFAShape fillRed:0.2f green:0.4f blue:1.0f alpha:0.66f];
    for(int i = 0; i < pointCount-1; i++) {
        [CFAShape ellipseWithXPos:xvals[i] yPos:yvals[i] width:i/2 andHeight:i/2];
    }
    [CFAShape stroke];
    [CFAShape strokeRed:1.0f green:0.2f blue:0.2f alpha:0.66f];
    [CFAShape ellipseWithXPos:xvals[pointCount-1] 
                         yPos:yvals[pointCount-1] 
                        width:pointCount/2 
                    andHeight:pointCount/2];
    [CFAShape noStroke];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Easing Circle</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/EasingCircle/</id>
   <updated>2011-08-19T04:17:52+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/EasingCircle/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/91.EasingCircle/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

NSPoint current, target;
float easing = 0.05f;

@implementation CFACanvas

-(void)setup {
    [self drawStyle:ANIMATED];
    [self windowWidth:400 andHeight:320];
    [CFAShape noStroke];
}

-(void)draw {
    [self background:0.3f alpha:0.05f];
    
    target.x = mousePos.x;
    float dx = target.x - current.x;
    if([CFAMath abs:dx] > 1) {
        current.x += dx * easing;
    }
    
    target.y = mousePos.y;
    float dy = target.y - current.y;
    if([CFAMath abs:dy] > 1) {
        current.y += dy * easing;
    }
    [CFAShape fillRed:1.0f green:0.2f blue:0.2f alpha:0.5f];
    [CFAShape circleAt:current radius:20];
    [CFAShape fillRed:0.2f green:0.4f blue:1.0f alpha:0.5f];
    [CFAShape circleAt:current radius:10];
}

@end
]]></script>

          
      </div>
   </content>
</entry>
  
  g<entry>
   <title>Constrained Circle</title>
   <id>tag:cocoaforartists.org,2011:/examples/KeyboardMouse/ConstrainedCircle/</id>
   <updated>2011-08-19T04:18:17+10:00</updated>
   <summary>@page_description</summary>
   <link rel="alternate" type="text/html" href="http://cocoaforartists.org/examples/KeyboardMouse/ConstrainedCircle/" />
   <content type="xhtml" xml:lang="en">
      <div xmlns="http://www.w3.org/1999/xhtml">
            <img src="../content/98.examples/992.KeyboardMouse/90.ConstrainedCircle/01.png" alt="01."/>

          
            <script type="syntaxhighlighter" class="brush: objc"><![CDATA[
#import "CFACanvas.h"

float mx, my, easing, esize;
int box;

@implementation CFACanvas

-(void)setup {
    [self drawStyle:ANIMATED];
    [self windowWidth:400 andHeight:320];
    [self background:0];
    [CFAShape stroke];
    [CFAShape strokeWidth:15.0f];
    easing = 0.05;
    esize = 50.0;   
    box = 30;
}

-(void)draw {
    [self background:0];
    if([CFAMath absf:mousePos.x - mx] > 0.1) mx = mx +(mousePos.x-mx)*easing;
    if([CFAMath absf:mousePos.y - my] > 0.1) my = my +(mousePos.y-my)*easing;
    
    mx = [CFAMath constrain:mx 
                        min:box+esize/2 
                        max:self.canvasWidth-box-esize/2];
    my = [CFAMath constrain:my 
                        min:box+esize/2 
                        max:self.canvasHeight-box-esize/2];
    
    [CFAShape noStroke];
    [CFAShape fill:0.3f];
    [CFAShape rectWithXPos:3*box/4 
                      yPos:3*box/4 
                     width:self.canvasWidth-3*box/2+1 
                 andHeight:self.canvasHeight-3*box/2+1];
    
    [CFAShape stroke];
    [CFAShape strokeRed:0.2f 
                  green:0.4f 
                   blue:1.0f];
    [CFAShape fillRed:1.0f 
                green:0.2f 
                 blue:0.2f];
    
    [CFAShape ellipseWithXPos:mx 
                         yPos:my 
                        width:esize 
                    andHeight:esize];
}
@end]]></script>

          
      </div>
   </content>
</entry>
  

